#include<iostream>
using namespace std;
#include<cmath>
#include<climits>

int main()
{
    int n,maxno,minno;
    cin>>n;
    maxno=INT_MIN;
    minno=INT_MAX;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        int n1;
        cin>>n1;
        arr[i]=n1;
    }
    for(int i=0;i<n;i++)
    {
        maxno=max(maxno,arr[i]);
        minno=min(minno,arr[i]);
    }
    cout<<maxno<<" "<<minno<<" ";
    return 0;
}
