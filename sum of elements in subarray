#include<iostream>
using namespace std;

int main()
{
    //sum of all subarrays
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    
    for(int i=0;i<n;i++)
    {
        int currsum=0;
        for(int j=i;j<n;j++)
        {
            currsum+=arr[j];
            cout<<currsum<<" ";
        }
    }
    return 0;
}
