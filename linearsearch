#include <iostream>

using namespace std;

//linear search
int linearsearch(int arr[],int n,int key)
{
    for(int i=0;i<n;i++)
    {
        if(arr[i]==key)
        {
            cout<<"the index no of given element is "<<i;
        }
    }
    return -1;
}
int main()
{
    int n,key;
    cout<<"enter no of elements in array:-";
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    cout<<"enter element to be searched:-";
    cin>>key;
    linearsearch(arr,n,key);
    return 0;
}
